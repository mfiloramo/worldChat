{
  "name": "worldChat",
  "version": "1.0.0",
  "description": "A simple app to have WebSocket-powered multilingual chat conversations with others",
  "main": "/client/index.js",
  "scripts": {
    "start": "node server/server.js",
    "build": "webpack",
    "dev": "concurrently \"npm run watch-css\" \"cross-env NODE_ENV=development webpack-dev-server --open --hot --progress --color\" \"nodemon ./server/server.js\"",
    "build-css": "tailwindcss build -i ./client/stylesheets/styles.css -o ./client/stylesheets/output.css",
    "watch-css": "tailwindcss build -i ./client/stylesheets/styles.css -o ./client/stylesheets/output.css --watch"
  },
  "author": "Michael Filoramo",
  "dependencies": {
    "-": "^0.0.1",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "D": "^1.0.0",
    "docker": "^1.0.0",
    "dotenv-webpack": "^8.0.0",
    "express": "^4.18.1",
    "jest": "^28.1.3",
    "pg": "^8.7.3",
    "postcss-preset-env": "^7.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "socket.io": "^4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "autoprefixer": "^10.4.8",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.19",
    "postcss": "^8.4.16",
    "postcss-loader": "^7.0.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.1.8",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
